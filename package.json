{
  "name": "playground",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "dist/src/handlers/index.js",
  "scripts": {
    "dev": "tsc -w -p tsconfig.json",
    "build": "tsc -p tsconfig.json",
    "setup:catalog": "ts-node --project tsconfig.json scripts/setup-catalog-table.ts",
    "setup:catalog:prod": "node dist/scripts/setup-catalog-table.js",
    "seed:genres": "ts-node --project tsconfig.json scripts/seed-genres.ts",
    "seed:genres:prod": "node dist/scripts/seed-genres.js",
    "seed:artists": "ts-node --project tsconfig.json scripts/seed-artists.ts",
    "seed:artists:prod": "node dist/scripts/seed-artists.js",
    "seed:all": "npm run seed:genres && npm run seed:artists",
    "seed:all:prod": "npm run seed:genres:prod && npm run seed:artists:prod",
    "test": "jest --runInBand",
    "lint": "eslint --ext .ts --ext .js .",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.933.0",
    "@aws-sdk/client-dynamodb": "^3.587.0",
    "@aws-sdk/lib-dynamodb": "^3.933.0",
    "dotenv": "^16.4.5",
    "i18next": "^25.6.2",
    "joi": "^18.0.1",
    "js-yaml": "^3.14.2",
    "pino": "^10.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.158",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
