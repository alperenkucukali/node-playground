{
  "name": "playground",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "dist/src/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --project tsconfig.json src/server.ts",
    "build": "tsc -p tsconfig.json",
    "prestart": "npm run build",
    "start": "node dist/src/server.js",
    "start:prod": "node dist/src/server.js",
    "setup:catalog": "ts-node --project tsconfig.json scripts/setup-catalog-table.ts",
    "setup:catalog:prod": "node dist/scripts/setup-catalog-table.js",
    "seed:genres": "ts-node --project tsconfig.json scripts/seed-genres.ts",
    "seed:genres:prod": "node dist/scripts/seed-genres.js",
    "seed:artists": "ts-node --project tsconfig.json scripts/seed-artists.ts",
    "seed:artists:prod": "node dist/scripts/seed-artists.js",
    "seed:all": "npm run seed:genres && npm run seed:artists",
    "seed:all:prod": "npm run seed:genres:prod && npm run seed:artists:prod",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.587.0",
    "@aws-sdk/lib-dynamodb": "^3.587.0",
    "dotenv": "^16.4.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
